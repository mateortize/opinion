- if !question.new_record?
  .panel.panel-default
    .panel-heading
      %h2.panel-title
        %a{ data: { parent: '#accordion', toggle: 'collapse'}, href: '#collapseTwo' } Answers
    #collapseTwo.panel-collapse.collapse
      .panel-body
        .row
          .matrix{ id: "question#{question.id}" }
            = render "answers_type#{question.question_type}", survey: survey, question: question
            %span.clearfix

  #modal.modal.fade{ role: 'dialog' }
    .modal-dialog
      .modal-content